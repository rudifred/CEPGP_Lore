					["RELIC"] =					"Relic",
					["THROWN"] =				"Thrown",
					["2HWEAPON"] =				"Two-Handed Weapon"
				}
				
				local slot = strsub(self:GetName(), 15, strfind(self:GetName(), "_weight")-1);
				
				fString:SetText(slots[slot]);
				
				local topFrame = self:GetAttribute("topLevel");
				
				if topFrame then return; end
				
				local point, parent, relPoint, x, y = self:GetPoint();
				y = y - fString:GetStringHeight();
				
				self:SetPoint(point, parent, relPoint, x, y);
